{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "RuntimeMinification": {
        "useInMemoryCache": true,
        "CacheBuster": "AppDomain"
      },
      "Runtime": {
        "Mode": "Production"
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://mootown.org/"
      },
      "Examine": {
        "LuceneDirectoryFactory": "TempFileSystemDirectoryFactory"
      }
    }
  }
}